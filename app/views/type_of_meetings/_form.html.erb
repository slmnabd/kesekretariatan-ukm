<div class="col-md-6 col-md-offset-3">
  <%= form_for(@type_of_meeting) do |f| %>

    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= f.submit "Submit", class: 'btn btn-primary form-control' %>
    </div>

  <% end %>
</div>